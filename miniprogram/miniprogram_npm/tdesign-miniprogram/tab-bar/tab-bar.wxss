.t-float-left {
  float: left;
}
.t-float-right {
  float: right;
}
@keyframes tdesign-fade-out {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
.hotspot-expanded.relative {
  position: relative;
}
.hotspot-expanded::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  transform: scale(1.5);
}
.t-tab-bar {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  position: relative;
  font-size: 16px;
  background-color: var(--td-tab-bar-bg-color, #fff);
  box-sizing: border-box;
}
.t-tab-bar--border::before {
  z-index: 1;
  position: absolute;
  box-sizing: border-box;
  content: ' ';
  pointer-events: none;
  right: 0;
  left: 0;
  top: 0;
  border-top: 1px solid var(--td-tab-bar-border-color, #e6e6e6);
  transform: scaleY(0.5);
}
.t-tab-bar--fixed {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
}
.t-tab-bar--safe {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.t-tab-bar--round {
  margin-left: 32rpx;
  margin-right: 32rpx;
  border-radius: 999px;
  box-shadow: 0px 6px 30px 5px rgba(0, 0, 0, 0.05), 0px 16px 24px 2px rgba(0, 0, 0, 0.04), 0px 8px 10px -5px rgba(0, 0, 0, 0.08);
}
